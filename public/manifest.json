{
  "name": "Nautilus",
  "short_name": "Nautilus",
  "version": "0.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "48",
  "description": "A light wallet for Ergo Platform",
  "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self';",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Nautilus"
  },
  "icons": {
    "48": "icons/app/48.png",
    "128": "icons/app/128.png",
    "512": "icons/app/512.png"
  }
}
